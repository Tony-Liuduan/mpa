{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "indent": [1, 4],
        "max-len": [1, 120],
        "no-console": 0,
        "no-restricted-syntax": 0,
        "consistent-return": 0,
        "class-methods-use-this": 0,
        "import/prefer-default-export": 0
    }
}